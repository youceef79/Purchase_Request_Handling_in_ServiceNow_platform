<name>catalog_script_client_b430fbd4477a0110a06c690cd36d43a1</name>
<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update sys_domain="global" table="catalog_script_client"&gt;&lt;catalog_script_client action="INSERT_OR_UPDATE"&gt;&lt;active&gt;true&lt;/active&gt;&lt;applies_catalog&gt;true&lt;/applies_catalog&gt;&lt;applies_extended&gt;false&lt;/applies_extended&gt;&lt;applies_req_item&gt;true&lt;/applies_req_item&gt;&lt;applies_sc_task&gt;false&lt;/applies_sc_task&gt;&lt;applies_target_record&gt;false&lt;/applies_target_record&gt;&lt;applies_to&gt;item&lt;/applies_to&gt;&lt;cat_item display_value="Matériel informatique"&gt;0f7bafd0477a0110a06c690cd36d4330&lt;/cat_item&gt;&lt;cat_variable&gt;IO:015ce314477a0110a06c690cd36d439a&lt;/cat_variable&gt;&lt;condition/&gt;&lt;description/&gt;&lt;field/&gt;&lt;global&gt;true&lt;/global&gt;&lt;isolate_script&gt;true&lt;/isolate_script&gt;&lt;messages/&gt;&lt;name&gt;infoItemControls&lt;/name&gt;&lt;order/&gt;&lt;script&gt;&lt;![CDATA[function onChange(control, oldValue, newValue, isLoading) {

    if (isLoading || newValue == '') {
		g_form.setValue("type_of_request","");
        return;
    }

    var options = initOptions();
	

    g_form.setMandatory("request_subtype", true);
    g_form.setDisplay("request_subtype", true);

    for (var i = 0; i &lt; options.length; i++) {
        g_form.addOption("request_subtype", options[i].value, options[i].label, i);
    }

    if (newValue == 'employee_request' || newValue == 'delegate_request') {

        for (i = 0; i &lt; options.length; i++) {

            if (options[i].value === 'ad_hoc') {
                g_form.removeOption("request_subtype", options[i].value);
            }
        }

    } else if (newValue == 'specific_request') {

        for (i = 0; i &lt; options.length; i++) {

            if (options[i].value === 'ad_hoc' ||
                options[i].value === 'standard' || options[i].value === '') {
                // nothing
            } else
                g_form.removeOption("request_subtype", options[i].value);

        }
    } else if (newValue == 'exceptional_request' || newValue == 'quotation_request') {
        g_form.setMandatory("request_subtype", false);
        g_form.setDisplay("request_subtype", false);
    }
}

function initOptions() {
    return [{
            label: "Astreinte",
            value: "astreint"
        },
        {
            label: "Facturé",
            value: "facture"
        },
        {
            label: "BCP",
            value: "bcp"
        },
        {
            label: "Prêt",
            value: "pret"
        },
        {
            label: "Standard",
            value: "standard"
        },
        {
            label: "Ad hoc",
            value: "ad_hoc"
        },
        {
            label: "Perte",
            value: "perte"
        },
        {
            label: "Vol",
            value: "vol"
        }
    ];
}]]&gt;&lt;/script&gt;&lt;sys_class_name&gt;catalog_script_client&lt;/sys_class_name&gt;&lt;sys_created_by&gt;admin&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2022-04-08 00:47:10&lt;/sys_created_on&gt;&lt;sys_domain&gt;global&lt;/sys_domain&gt;&lt;sys_domain_path&gt;/&lt;/sys_domain_path&gt;&lt;sys_id&gt;b430fbd4477a0110a06c690cd36d43a1&lt;/sys_id&gt;&lt;sys_mod_count&gt;25&lt;/sys_mod_count&gt;&lt;sys_name&gt;infoItemControls&lt;/sys_name&gt;&lt;sys_overrides/&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;catalog_script_client_b430fbd4477a0110a06c690cd36d43a1&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;admin&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2022-04-09 21:57:39&lt;/sys_updated_on&gt;&lt;table/&gt;&lt;type&gt;onChange&lt;/type&gt;&lt;ui_type&gt;10&lt;/ui_type&gt;&lt;variable_set/&gt;&lt;view/&gt;&lt;/catalog_script_client&gt;&lt;/record_update&gt;</payload>
